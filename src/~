####### Compile

commands.o: commands.cpp commands.h \
		music.h \
		myClock.h \
		network/websearch.h \
		user.h \
		Personality.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o commands.o commands.cpp

commandtest.o: commandtest.cpp commands.h \
		music.h \
		myClock.h \
		network/websearch.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o commandtest.o commandtest.cpp

music.o: music.cpp music.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o music.o music.cpp

myClock.o: myClock.cpp myClock.h \
		music.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o myClock.o myClock.cpp

Personality.o: Personality.cpp Personality.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Personality.o Personality.cpp

user.o: user.cpp user.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o user.o user.cpp

test.o: network/test.cpp network/websearch.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o test.o network/test.cpp

websearch.o: network/websearch.cpp network/websearch.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o websearch.o network/websearch.cpp

hello.o: speech/hello.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o hello.o speech/hello.cpp

main.o: gui/jTest/main.cpp gui/jTest/simplemenu.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o gui/jTest/main.cpp

simplemenu.o: gui/jTest/simplemenu.cpp gui/jTest/simplemenu.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o simplemenu.o gui/jTest/simplemenu.cpp

pushbutton.o: gui/oldFiles/pushbutton.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o pushbutton.o gui/oldFiles/pushbutton.cpp

main.o: gui/plusminus/main.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o gui/plusminus/main.cpp

