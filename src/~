dist: distdir FORCE
	(cd `dirname $(DISTDIR)` && $(TAR) $(DISTNAME).tar $(DISTNAME) && $(COMPRESS) $(DISTNAME).tar) && $(MOVE) `dirname $(DISTDIR)`/$(DISTNAME).tar.gz . && $(DEL_FILE) -r $(DISTDIR)

distdir: FORCE
	@test -d $(DISTDIR) || mkdir -p $(DISTDIR)
	$(COPY_FILE) --parents $(DIST) $(DISTDIR)/
	$(COPY_FILE) --parents commands.h music.h myClock.h Personality.h user.h network/websearch.h gui/jTest/simplemenu.h gui/plusminus/plusminus.h speech/pocketsphinx/include/cmdln_macro.h speech/pocketsphinx/include/pocketsphinx.h speech/pocketsphinx/include/pocketsphinx_export.h speech/pocketsphinx/include/ps_lattice.h speech/pocketsphinx/include/ps_mllr.h speech/pocketsphinx/include/ps_search.h speech/pocketsphinx/src/gst-plugin/gstpocketsphinx.h speech/pocketsphinx/src/libpocketsphinx/acmod.h speech/pocketsphinx/src/libpocketsphinx/allphone_search.h speech/pocketsphinx/src/libpocketsphinx/bin_mdef.h speech/pocketsphinx/src/libpocketsphinx/blkarray_list.h speech/pocketsphinx/src/libpocketsphinx/dict.h speech/pocketsphinx/src/libpocketsphinx/dict2pid.h speech/pocketsphinx/src/libpocketsphinx/fsg_history.h speech/pocketsphinx/src/libpocketsphinx/fsg_lextree.h speech/pocketsphinx/src/libpocketsphinx/fsg_search_internal.h speech/pocketsphinx/src/libpocketsphinx/hmm.h speech/pocketsphinx/src/libpocketsphinx/kws_detections.h speech/pocketsphinx/src/libpocketsphinx/kws_search.h speech/pocketsphinx/src/libpocketsphinx/mdef.h speech/pocketsphinx/src/libpocketsphinx/ms_gauden.h speech/pocketsphinx/src/libpocketsphinx/ms_mgau.h speech/pocketsphinx/src/libpocketsphinx/ms_senone.h speech/pocketsphinx/src/libpocketsphinx/ngram_search.h speech/pocketsphinx/src/libpocketsphinx/ngram_search_fwdflat.h speech/pocketsphinx/src/libpocketsphinx/ngram_search_fwdtree.h speech/pocketsphinx/src/libpocketsphinx/phone_loop_search.h speech/pocketsphinx/src/libpocketsphinx/pocketsphinx_internal.h speech/pocketsphinx/src/libpocketsphinx/ps_alignment.h speech/pocketsphinx/src/libpocketsphinx/ps_lattice_internal.h speech/pocketsphinx/src/libpocketsphinx/ptm_mgau.h speech/pocketsphinx/src/libpocketsphinx/s2_semi_mgau.h speech/pocketsphinx/src/libpocketsphinx/s3types.h speech/pocketsphinx/src/libpocketsphinx/state_align_search.h speech/pocketsphinx/src/libpocketsphinx/tied_mgau_common.h speech/pockets